<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stx.mapper.fxyjMapper">


    <delete id="deleteFxyj" parameterType="domain.Fxyj">
        delete from Fxyj where fxid=#{fxid};
    </delete>



    <select id="findAll" resultType="domain.Fxyj">
         select * from Fxyj
    </select>

</mapper>